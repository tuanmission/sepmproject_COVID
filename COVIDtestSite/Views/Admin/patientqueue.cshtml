@model COVIDtestSite.ViewModels.QueueViewModel;
@{
    ViewData["Title"] = "Queue";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Patients in Queue</h1>
<div class="table-responsive">
    <table class="table table-hover" id="testtable">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Address</th>
                <th scope="col">Phone Number</th>
                <th scope="col">Medicare Number</th>
            </tr>

        </thead>
        <tbody>
            <tr></tr>
        </tbody>

    </table>
</div>
@section Scripts{
    <script>
        $(document).ready(function () {

            var table = $("#testtable").DataTable({
                ajax: {

                    url: "/api/Admin/patientqueue/@Model.TestSiteId",
                    dataSrc: "",

                },
                columns: [
                    {
                        data: "name"
                    },
                    {
                        data: "address"
                    },
                    {
                        data: "phoneNumber"

                    },
                    {
                        data:"medicareNumber"
                    }

                ]

            });


            });











    </script>

}
